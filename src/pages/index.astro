---
import MainLayout from "../layouts/MainLayout.astro";
import ArticlesSection from "../components/ArticlesSection.astro";
import type { ArticleProps } from '../utils/types';
import type { MarkdownInstance } from "astro";

const allArticles: MarkdownInstance<ArticleProps>[] = await Astro.glob("../content/articles/*.md");
---

<MainLayout title="Welcome to Astro.">
  <!-- <SearchBar /> -->
  <ArticlesSection allArticles={allArticles} />
</MainLayout>
